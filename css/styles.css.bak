/** Button language select **/
/** ################################**/
.button {
	position:absolute;
	left:85%;
  display: inline-block;
  border-radius: 4px;
  background-color: #DABDAB;
  border: none;
  color: LightSlateGrey;
  text-align: center;
  font-size: .7em;
   width: 25px;
  height: 15px;
  transition: all 0.5s;
  margin: 5px;
  box-shadow: 0px 2px 5px 1px LightSlateGrey;
}



.button span {
  cursor: pointer;
  display: inline-block;
  position: relative;
  transition: 0.5s;
}

.button span:after {
  content: '\00bb';
  position: absolute;
  opacity: 0;
  top: 0;
  right: -20px;
  transition: 0.5s;
}

.button:hover span {
  padding-right: 10px;
}

.button:hover span:after {
  opacity: 1;
  right: 0;
}


#button_switch{
	position:absolute;
	left:1%;
  display: inline-block;
  border-radius: 4px;
  background-color: #DABDAB;
  border: none;
  color: LightSlateGrey;
  text-align: center;
  font-weight:700;
   width: auto;
  height: auto;
  margin: 1px;
  box-shadow: 0.5px 0.5px 1px 1px LightSlateGrey;
}

#menu-button{
	  color: Ivory;
}
#menu-button:hover, #menu-button:focus, #menu-button:active {
	  color: #DABDAB;
}


/** Button language select END **/
